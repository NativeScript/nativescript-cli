{
  "name": "nativescript",
  "preferGlobal": true,
  "version": "7.0.5-rc.0",
  "author": "NativeScript <support@nativescript.org>",
  "description": "Command-line interface for building NativeScript projects",
  "bin": {
    "tns": "./bin/tns",
    "nativescript": "./bin/tns",
    "nsc": "./bin/tns",
    "ns": "./bin/tns"
  },
  "main": "./lib/nativescript-cli-lib.js",
  "files": [
    "bin/nativescript.js",
    "config",
    "docs",
    "!docs/html",
    "lib",
    "!lib/**/*.ts",
    "!lib/**/*.js.map",
    "!lib/common/test",
    "!lib/common/docs/fonts",
    "resources",
    "setup",
    "vendor",
    "postinstall.js",
    "preuninstall.js"
  ],
  "scripts": {
    "clean": "npx rimraf node_modules package-lock.json && npm run setup",
    "build": "grunt",
    "build.all": "grunt test",
    "setup": "npm i --ignore-scripts && npm run build",
    "test": "mocha --config=test/.mocharc.yml",
    "postinstall": "node postinstall.js",
    "preuninstall": "node preuninstall.js",
    "prepack": "grunt prepare",
    "postpack": "grunt set_dev_ga_id",
    "mocha": "mocha",
    "tsc": "tsc",
    "tslint": "tslint -p tsconfig.json",
    "test-watch": "node ./dev/tsc-to-mocha-watch.js",
    "tslint-fix": "tslint -p tsconfig.json --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prettier": "prettier --write ./lib/**/*{.ts,.d.ts} ./test/**/*{.ts,.d.ts}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NativeScript/nativescript-cli.git"
  },
  "keywords": [
    "nativescript",
    "telerik",
    "mobile"
  ],
  "dependencies": {
    "@nativescript/doctor": "^2.0.3",
    "@nativescript/schematics-executor": "0.0.2",
    "byline": "5.0.0",
    "chalk": "4.1.0",
    "chokidar": "3.4.1",
    "cli-table": "https://github.com/telerik/cli-table/tarball/v0.3.1.2",
    "color": "3.1.2",
    "colors": "1.4.0",
    "convert-source-map": "1.7.0",
    "detect-newline": "3.1.0",
    "email-validator": "2.0.4",
