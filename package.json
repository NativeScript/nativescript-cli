{
  "name": "kinvey-js-sdk",
  "version": "3.5.0",
  "description": "Kinvey JavaScript SDK for developing JavaScript applications. This is the core code used to create specific bundles for all other JavaScript platforms.",
  "homepage": "http://www.kinvey.com",
  "bugs": {
    "url": "https://github.com/Kinvey/js-sdk/issues"
  },
  "license": "Apache-2.0",
  "author": "Kinvey, Inc.",
  "main": "./dist/index.js",
  "es6": true,
  "contributors": [
    "Thomas Conner <thomas@kinvey.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Kinvey/js-sdk"
  },
  "scripts": {
    "build": "npm run clean && npm run transpile",
    "clean": "del dist coverage",
    "cover": "istanbul cover _mocha -- --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/index test",
    "docs": "esdoc -c esdoc.json",
    "lint": "npm run lint:src",
    "lint:src": "eslint src/**",
    "lint:test": "eslint test/**",
    "preversion": "del node_modules && npm install && npm test",
    "postversion": "git push && git push --tags",
    "test": "mocha --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/index test",
    "test:watch": "mocha -w --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/index test",
    "transpile": "BABEL_ENV=production babel --no-comments --out-dir dist src"
  },
  "dependencies": {
    "append-query": "2.0.1",
    "core-js": "2.4.1",
    "es6-error": "4.0.2",
    "es6-promise": "4.1.0",
    "fast-memory-cache": "2.0.4",
    "hellojs": "1.14.1",
    "local-storage": "1.4.2",
    "lodash": "4.17.4",
    "loglevel": "1.4.1",
    "promise-queue": "2.2.3",
    "qs": "6.4.0",
    "request": "2.81.0",
    "rxjs": "5.2.0",
    "sift": "3.2.7",
    "uid": "0.0.2",
    "url-pattern": "1.0.3"
  },
  "devDependencies": {
    "app-module-path": "2.2.0",
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-inline-dotenv": "^1.1.1",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "del-cli": "^0.2.0",
    "esdoc": "0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "3.17.1",
    "eslint-config-airbnb-base": "11.1.1",
    "eslint-import-resolver-babel-module": "3.0.0",
    "eslint-plugin-import": "2.2.0",
    "expect": "^1.20.2",
    "istanbul": "1.1.0-alpha.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.0",
    "nock": "9.0.9",
    "regenerator-runtime": "^0.9.5"
  },
  "engines": {
    "node": "6.9.1"
  },
  "keywords": [
    "Kinvey",
    "JavaScript",
    "SDK"
  ]
}
