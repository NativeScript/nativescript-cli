{
  "name": "kinvey-javascript-sdk-core",
  "version": "3.0.0-beta.25",
  "description": "Kinvey JavaScript SDK Core",
  "main": "./es5/kinvey.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test",
    "test:watch": "./node_modules/mocha/bin/mocha -w --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test",
    "test:jenkins": "istanbul cover _mocha -- --reporter tap --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test > test.tap && istanbul report clover",
    "build": "./node_modules/.bin/gulp build",
    "lint": "./node_modules/.bin/gulp lint",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag $PACKAGE_VERSION && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kinvey/javascript-sdk-core.git"
  },
  "author": "Kinvey, Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Kinvey/javascript-sdk-core/issues"
  },
  "dependencies": {
    "append-query": "^1.1.0",
    "babel-regenerator-runtime": "^6.5.0",
    "es6-error": "^3.0.0",
    "es6-symbol": "^3.1.0",
    "fast-memory-cache": "^2.0.4",
    "hellojs": "^1.13.1",
    "indexeddbshim": "^2.2.1",
    "local-storage": "^1.4.2",
    "lodash": "^4.12.0",
    "loglevel": "^1.4.1",
    "promise-queue": "^2.2.3",
    "qs": "^6.2.0",
    "rxjs": "^5.0.0-beta.7",
    "sift": "^3.0.0",
    "uid": "0.0.2",
    "url-pattern": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.1.0",
    "chai-as-promised": "^5.1.0",
    "del": "^2.2.0",
    "esdoc": "^0.4.7",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "fake-indexeddb": "^1.0.6",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-bump": "^2.1.0",
    "gulp-env": "^0.4.0",
    "gulp-eslint": "^2.0.0",
    "gulp-prompt": "^0.2.0",
    "gulp-s3-upload": "^1.6.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tag-version": "^1.3.0",
    "gulp-util": "^3.0.7",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "request": "^2.72.0",
    "semver": "^5.1.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "uid": "0.0.2",
    "webpack": "^1.13.1",
    "webpack-stream": "^3.2.0",
    "yargs": "^4.7.1"
  }
