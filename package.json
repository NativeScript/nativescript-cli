    "ios-sim-portable": "4.2.0",
    "istextorbinary": "5.9.0",
    "jimp": "0.14.0",
    "libnpmconfig": "1.2.1",
    "lodash": "4.17.19",
    "log4js": "6.3.0",
    "marked": "1.1.1",
    "marked-terminal": "https://github.com/NativeScript/marked-terminal/tarball/v3.1.1n",
    "minimatch": "3.0.4",
    "mkdirp": "1.0.4",
    "mute-stream": "0.0.8",
    "nativescript-dev-xcode": "0.2.1",
    "nativescript-preview-sdk": "0.4.2",
    "open": "7.1.0",
    "ora": "5.0.0",
    "osenv": "0.1.5",
    "pacote": "11.1.11",
    "pako": "1.0.11",
    "pbxproj-dom": "1.2.0",
    "plist": "3.0.1",
    "plist-merge-patch": "0.1.1",
    "prettier": "2.1.1",
    "proper-lockfile": "4.1.1",
    "proxy-lib": "0.4.0",
    "qr-image": "3.2.0",
