{
  "name": "@akylas/nativescript-cli",
  "main": "./lib/nativescript-cli-lib.js",
  "version": "8.8.3",
  "author": "NativeScript <support@nativescript.org>",
  "description": "Command-line interface for building NativeScript projects",
  "bin": {
    "nativescript": "./bin/tns",
    "ns": "./bin/tns",
    "nsc": "./bin/tns",
    "tns": "./bin/tns"
  },
  "files": [
    "bin/*",
    "config",
    "docs",
    "!docs/html",
    "lib",
    "!lib/**/*.ts",
    "lib/**/*.d.ts",
    "!lib/**/*.js.map",
    "!lib/common/test",
    "!lib/common/docs/fonts",
    "!node_modules",
    "resources",
    "setup",
    "vendor",
    "postinstall.js",
    "preuninstall.js"
  ],
  "scripts": {
    "clean": "npx rimraf node_modules package-lock.json && npm run setup",
    "build": "grunt",
    "build.all": "grunt test",
    "dev": "tsc --watch",
    "setup": "npm i --ignore-scripts && npx husky install",
    "test": "npm run tsc && mocha --config=test/.mocharc.yml",
    "postinstall": "node postinstall.js",
    "preuninstall": "node preuninstall.js",
    "prepack": "grunt prepare",
    "pack": "grunt pack",
    "mocha": "mocha",
    "tsc": "tsc",
    "test-watch": "node ./dev/tsc-to-mocha-watch.js",
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prettier": "prettier --write ./lib/**/*{.ts,.d.ts} ./test/**/*{.ts,.d.ts}"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NativeScript/nativescript-cli.git"
  },
  "keywords": [
    "nativescript",
    "telerik",
    "mobile"
  ],
  "dependencies": {
    "@nativescript/doctor": "2.0.15",
    "@nativescript/schematics-executor": "0.0.2",
    "@npmcli/arborist": "^7.2.0",
